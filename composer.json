{
    "name": "sigmie/poll-ops",
    "description": "PHP package for handling polled operations'",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nico",
            "email": "nico@sigmie.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sigmie\\PollOps\\": "src/"
        },
        "files": [
            "src/functions.php"
        ],
        "exclude-from-classmap": [
            "tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Sigmie\\PollOps\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.66",
        "phpunit/phpunit": "^9.1",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/var-dumper": "^5.0"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
